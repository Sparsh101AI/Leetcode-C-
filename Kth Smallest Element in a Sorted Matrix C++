class Solution {
public:
    int kthSmallest(vector<vector<int>>& matrix, int k) {
        vector<int> holder;
        for(int i = 0; i < matrix.size(); i++ ){
            for(int k = 0; k < matrix[i].size(); k++){
                holder.push_back(matrix[i][k]);
            }
        }

        sort(holder.begin(),holder.end());

        return holder[k-1];





    }
};
